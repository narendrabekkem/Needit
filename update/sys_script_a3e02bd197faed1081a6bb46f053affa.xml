        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {
    //loading scratchpad object for StateModel
	gs.include('hasActiveFlow');
	prevobj={};
	for (var key in previous)
		{
			prevobj[key]=previous.getValue(key);
		}
	gs.addInfoMessage('Who ever said Display Br has no access to previous object. Here you go \n'+JSON.stringify(prevobj));
	var msg='Display BR is triggered and scratchpad object id being populated';
	gs.addInfoMessage(msg);
    g_scratchpad.context_record = getActiveFlow(current);
	try{
	var confirmuserhasright=gs.getUser().isMemberOf(previous.assignment_group.name.toString());
    g_scratchpad.isMemberofAssgnGrp =confirmuserhasright;
	}catch(e)
	{
		gs.addInfoMessage(e.message());
	}
	gs.addInfoMessage('Is member of Group \t ' + g_scratchpad.isMemberofAssgnGrp);
	
})(current, previous);]]></script>
