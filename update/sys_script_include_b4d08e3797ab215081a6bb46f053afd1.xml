    <sys_script_include action="INSERT_OR_UPDATE">
        <access>package_private</access>
        <active>true</active>
        <api_name>x_58872_needit.GetGroupUsers</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description/>
        <name>GetGroupUsers</name>
        <script><![CDATA[var GetGroupUsers = Class.create();
GetGroupUsers.prototype = {
    initialize: function() {
    },
	users:function (group)
	{
		var gr=new GlideRecord('sys_user_grmember');
		users=[];
		gr.addQuery('group',group);
		gr.query();
		while(gr.next())
		{
			users.push(gr.user.toString());
		}

		return 'sys_idIN'+users;
	},

    type: 'GetGroupUsers'
};]]></script>
